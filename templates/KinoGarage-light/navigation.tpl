<div class="pagination ignore-select" id="pagination">
	[not-available=showfull]
	<div class="pagination__pages11 d-flex jc-center">
		<div class="pagination__pages-btn">[prev-link]<span class="fal fa-long-arrow-left"></span>[/prev-link]</div>
		<div class="pagination__pages">{pages}</div>
		<div class="pagination__pages-btn">[next-link]<span class="fal fa-long-arrow-right"></span>[/next-link]</div>
	</div>
	[/not-available]
	[available=showfull]
	<div class="pagination__pages pagination__pages--full d-flex jc-center">{pages}</div>
	[/available]
</div>